1.number = int(input("Enter a number: "))

for i in range(1, 11):
    print(number, "x", i, "=", number*i)
2.fruits = ["Apple", "Banana", "Cherry"]

for fruit in fruits:
    print(fruit)
3.for i in range(1, 21):
    if i % 2 == 0:
        print(i)
4.number = 5
factorial = 1

for i in range(1, number + 1):
    factorial *= i

print("Factorial of", number, "is", factorial)
5.number = 29
is_prime = True

for i in range(2, number):
    if number % i == 0:
        is_prime = False
        break

if is_prime:
    print(number, "is prime")
else:
    print(number, "is not prime")
6.string = "Artificial Intelligence and Machine Learning"
vowels = "aeiouAEIOU"
count = 0

for char in string:
    if char in vowels:
        count += 1

print("Number of vowels in the string:", count)
